{
    "version": 2,
    "installCommand": "npm install --legacy-peer-deps",
    "buildCommand": "CI=false GENERATE_SOURCEMAP=false react-scripts build",
    "outputDirectory": "build",
    "framework": "create-react-app",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}